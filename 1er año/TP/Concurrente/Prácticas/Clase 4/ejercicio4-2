programa Ejercicio3-1

areas
  ciudad : AreaC(1,1,100,100)

robots 
  robot tipo1
  comenzar
    mover
    BloquearEsquina(10,10)
    Pos (10,10)
    mover
  fin

  robot tipo2
  comenzar
    mover
    derecha
    mover
    BloquearEsquina(10,10)
    Pos (10,10)
    LiberarEsquina(10,10)
  fin

variables 
  robot1: tipo1
  robot2: tipo2
  
comenzar 
  AsignarArea(robot1,ciudad)
  AsignarArea(robot2,ciudad)
  
  Iniciar(robot1, 2, 2)
  Iniciar(robot2, 6, 2) 
fin