Object subclass: #Balanza	instanceVariableNames: 'cantidadDeProductos precioTotal pesoTotal'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-BalanzaElectronica'!!Balanza methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:20'!precioTotal	"comment stating purpose of message"	^precioTotal ! !!Balanza methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:19'!cantidadDeProductos	"comment stating purpose of message"	^cantidadDeProductos ! !!Balanza methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:20'!pesoTotal	"comment stating purpose of message"	^pesoTotal! !!Balanza methodsFor: 'actions' stamp: 'JulianCasaburi 8/30/2018 15:35'!ponerEnCero	"Reinicia los valores"	cantidadDeProductos := 0.	precioTotal := 0.0.	pesoTotal := 0.0.! !!Balanza methodsFor: 'actions' stamp: 'JulianCasaburi 8/29/2018 03:51'!emitirTicket	"Crea, inicializa y retorna un ticket"	| ticket |	ticket := Ticket new fecha: Date today.	ticket		cantidadDeProductos: self cantidadDeProductos;		pesoTotal: self pesoTotal;		precioTotal: self precioTotal.	^ticket! !!Balanza methodsFor: 'actions' stamp: 'JulianCasaburi 8/30/2018 15:36'!agregarProducto: producto	"Agrega producto a la Balanza, incrementando los valores de las variables de instancia"	cantidadDeProductos:= self cantidadDeProductos + 1.	precioTotal := self precioTotal + producto precio.	pesoTotal := self pesoTotal + producto peso.	! !!Balanza methodsFor: 'initialization' stamp: 'JulianCasaburi 8/30/2018 15:35'!initialize	"comment stating purpose of message"	cantidadDeProductos :=0.	precioTotal := 0.0.	pesoTotal := 0.0.! !Object subclass: #Producto	instanceVariableNames: 'descripcion peso precioPorKilo'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-BalanzaElectronica'!!Producto methodsFor: 'accessing' stamp: 'JulianCasaburi 8/28/2018 01:37'!peso	"comment stating purpose of message"	^peso! !!Producto methodsFor: 'accessing' stamp: 'JulianCasaburi 8/28/2018 01:42'!descripcion: unaDescripcion	"comment stating purpose of message"	descripcion := unaDescripcion.! !!Producto methodsFor: 'accessing' stamp: 'JulianCasaburi 8/28/2018 01:43'!precioPorKilo	"comment stating purpose of message"	^precioPorKilo.! !!Producto methodsFor: 'accessing' stamp: 'JulianCasaburi 8/28/2018 01:37'!peso: unPeso	"comment stating purpose of message"	peso := unPeso.! !!Producto methodsFor: 'accessing' stamp: 'JulianCasaburi 8/28/2018 01:44'!precioPorKilo: unPrecio	"comment stating purpose of message"	precioPorKilo := unPrecio ! !!Producto methodsFor: 'accessing' stamp: 'JulianCasaburi 8/28/2018 01:42'!descripcion	"comment stating purpose of message"	^descripcion. ! !!Producto methodsFor: 'as yet unclassified' stamp: 'JulianCasaburi 8/28/2018 01:47'!precio: unPrecio	"Asigna un precio al producto"	precioPorKilo := unPrecio / peso.! !!Producto methodsFor: 'as yet unclassified' stamp: 'JulianCasaburi 8/28/2018 01:47'!precio	"comment stating purpose of message"	^(precioPorKilo * peso)! !Object subclass: #Ticket	instanceVariableNames: 'fecha cantidadDeProductos pesoTotal precioTotal'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-BalanzaElectronica'!!Ticket methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:13'!cantidadDeProductos	"comment stating purpose of message"	^cantidadDeProductos ! !!Ticket methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:15'!pesoTotal	"comment stating purpose of message"	^pesoTotal! !!Ticket methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:13'!fecha: unaFecha	"comment stating purpose of message"	fecha:= unaFecha.! !!Ticket methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:15'!precioTotal	"comment stating purpose of message"	^precioTotal! !!Ticket methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:14'!pesoTotal: unPeso	"comment stating purpose of message"	pesoTotal := unPeso.! !!Ticket methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:14'!cantidadDeProductos: unaCantidad	"comment stating purpose of message"	cantidadDeProductos := unaCantidad.! !!Ticket methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:14'!precioTotal: unPrecio	"comment stating purpose of message"	precioTotal := unPrecio.! !!Ticket methodsFor: 'accessing' stamp: 'JulianCasaburi 8/29/2018 03:13'!fecha	"comment stating purpose of message"	^fecha! !!Ticket methodsFor: 'as yet unclassified' stamp: 'JulianCasaburi 8/29/2018 03:12'!impuesto	"comment stating purpose of message"	^ (precioTotal * 21) / 100! !