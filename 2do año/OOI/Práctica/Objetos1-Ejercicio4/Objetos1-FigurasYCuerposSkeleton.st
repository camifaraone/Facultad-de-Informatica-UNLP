TestCase subclass: #CirculoTest	instanceVariableNames: 'circuloRadioDos circuloRadioCuatro'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-FigurasYCuerposSkeleton-Tests'!!CirculoTest methodsFor: 'tests' stamp: 'casco 8/29/2018 21:54:44'!testArea	self assert: circuloRadioDos area rounded = 13.	self assert: circuloRadioCuatro area rounded = 50.	self deny: ((circuloRadioCuatro area < 50) or: circuloRadioCuatro area > 51).! !!CirculoTest methodsFor: 'tests' stamp: 'casco 8/29/2018 21:54:44'!testDiametro	self assert: circuloRadioDos diametro = 4.	self deny: circuloRadioCuatro diametro > 8.	self assert: circuloRadioCuatro diametro = 8.	! !!CirculoTest methodsFor: 'tests' stamp: 'casco 8/29/2018 21:54:44'!testPerimetro	self assert: circuloRadioDos perimetro rounded = 13.	self assert: circuloRadioCuatro perimetro rounded = 25.! !!CirculoTest methodsFor: 'tests' stamp: 'casco 8/29/2018 21:54:44'!setUp	circuloRadioCuatro := Circulo new radio: 4; yourself.	circuloRadioDos := Circulo new radio: 2; yourself.! !!CirculoTest methodsFor: 'tests' stamp: 'casco 8/29/2018 21:54:44'!testRadio	self assert: circuloRadioDos radio = 2.	circuloRadioDos radio: 3.	self assert: circuloRadioDos radio = 3.	self assert: circuloRadioCuatro radio = 4.! !TestCase subclass: #CuadradoTest	instanceVariableNames: 'cuadradoLadoDos cuadradoLadoCuatro'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-FigurasYCuerposSkeleton-Tests'!!CuadradoTest methodsFor: 'initialization' stamp: 'casco 8/29/2018 21:54:44'!setUp	cuadradoLadoDos := Cuadrado new lado: 2; yourself.	cuadradoLadoCuatro := Cuadrado new lado: 4; yourself.! !!CuadradoTest methodsFor: 'initialization' stamp: 'casco 8/29/2018 21:54:44'!testArea		self assert: cuadradoLadoDos perimetro = 8.	cuadradoLadoDos lado: 3.	self assert: cuadradoLadoDos perimetro = 12.	self assert: cuadradoLadoCuatro perimetro = 16.	! !!CuadradoTest methodsFor: 'initialization' stamp: 'casco 8/29/2018 21:54:44'!testLado	self assert: cuadradoLadoCuatro lado = 4.	cuadradoLadoCuatro lado: 8.	self assert: cuadradoLadoCuatro lado = 8.		self assert: cuadradoLadoDos lado = 2.! !!CuadradoTest methodsFor: 'initialization' stamp: 'casco 8/29/2018 21:54:44'!testPerimetro		self assert: cuadradoLadoDos perimetro = 8.	cuadradoLadoDos lado: 3.	self assert: cuadradoLadoDos perimetro = 12.	self assert: cuadradoLadoCuatro perimetro = 16.! !TestCase subclass: #CuerpoTest	instanceVariableNames: 'cilindro prisma'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-FigurasYCuerposSkeleton-Tests'!!CuerpoTest methodsFor: 'tests' stamp: 'casco 8/29/2018 21:54:44'!testVolumen	self assert: cilindro volumen rounded equals: 63.	cilindro altura: 10.	self assert: cilindro volumen rounded equals: 126.		prisma altura: 10.	self assert: prisma volumen rounded equals: 40.! !!CuerpoTest methodsFor: 'tests' stamp: 'casco 8/29/2018 21:54:44'!setUp	cilindro := Cuerpo new altura: 5; caraBasal: (Circulo new radio: 2); yourself. 	prisma := Cuerpo new altura: 7; caraBasal: (Cuadrado new lado: 2); yourself. ! !!CuerpoTest methodsFor: 'tests' stamp: 'casco 8/29/2018 21:54:44'!testAltura	self assert: prisma altura equals: 7.	self assert: cilindro altura equals: 5.		prisma altura: prisma altura * 2.	self assert: prisma altura equals: 14.! !!CuerpoTest methodsFor: 'tests' stamp: 'casco 8/29/2018 21:54:44'!testSuperficieExterior		self assert: prisma superficieExterior rounded equals: 64.	self assert: cilindro superficieExterior rounded equals: 88.! !