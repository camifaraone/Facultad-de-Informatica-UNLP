Object subclass: #Inversion	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-Ejercicio12'!!Inversion methodsFor: 'as yet unclassified' stamp: 'JulianCasaburi 1/7/2020 20:38'!valorActual	^ self subclassResponsibility! !Inversion subclass: #InversionEnAcciones	instanceVariableNames: 'nombreDeLaAccion cantidadDeAcciones valorActualPorAccion'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-Ejercicio12'!!InversionEnAcciones methodsFor: 'initialization' stamp: 'JulianCasaburi 9/21/2018 23:08'!initialize	"comment stating purpose of message"	nombreDeLaAccion := nil.	cantidadDeAcciones := 0.	valorActualPorAccion := 0.0.! !!InversionEnAcciones methodsFor: 'as yet unclassified' stamp: 'JulianCasaburi 9/21/2018 23:31'!valorActual	"comment stating purpose of message"	^cantidadDeAcciones * valorActualPorAccion! !!InversionEnAcciones methodsFor: 'private' stamp: 'JulianCasaburi 9/21/2018 23:08'!nombreDeLaAccion: unNombre	"comment stating purpose of message"	nombreDeLaAccion := unNombre.! !!InversionEnAcciones methodsFor: 'private' stamp: 'JulianCasaburi 9/21/2018 23:09'!cantidadDeAcciones: unaCantidad	"comment stating purpose of message"	cantidadDeAcciones := unaCantidad .! !!InversionEnAcciones methodsFor: 'private' stamp: 'JulianCasaburi 9/21/2018 23:09'!valorActualPorAccion: unValor	"comment stating purpose of message"	valorActualPorAccion := unValor.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!InversionEnAcciones class	instanceVariableNames: ''!!InversionEnAcciones class methodsFor: 'instance creation' stamp: 'JulianCasaburi 9/21/2018 23:19'!conNombre: unNombre conCantidad: unaCantidad conValorActualPorAccion: unValor	"comment stating purpose of message"	^self new		nombreDeLaAccion: unNombre;		cantidadDeAcciones: unaCantidad;		valorActualPorAccion: unValor;		yourself.! !Object subclass: #Inversor	instanceVariableNames: 'nombre inversiones'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-Ejercicio12'!!Inversor methodsFor: 'as yet unclassified' stamp: 'JulianCasaburi 9/22/2018 00:30'!valorActual	"comment stating purpose of message"	^inversiones inject: 0.0 into: [ :valorTotal :inversion | valorTotal + inversion valorActual ].! !!Inversor methodsFor: 'as yet unclassified' stamp: 'JulianCasaburi 9/21/2018 23:46'!agregarInversion: unaInversion	"comment stating purpose of message"	inversiones add: unaInversion .! !!Inversor methodsFor: 'private' stamp: 'JulianCasaburi 9/21/2018 23:29'!nombre: unNombre	"comment stating purpose of message"	nombre := unNombre .! !!Inversor methodsFor: 'initialization' stamp: 'JulianCasaburi 9/21/2018 23:27'!initialize	"comment stating purpose of message"	nombre:= nil.	inversiones := Set new.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Inversor class	instanceVariableNames: ''!!Inversor class methodsFor: 'as yet unclassified' stamp: 'JulianCasaburi 9/21/2018 23:28'!conNombre: unNombre	"comment stating purpose of message"	^ self new		nombre: unNombre;		yourself.! !Inversion subclass: #PlazoFijo	instanceVariableNames: 'fechaDeConstitucion montoDepositado porcentajeDeInteresDiario'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-Ejercicio12'!!PlazoFijo methodsFor: 'initialization' stamp: 'JulianCasaburi 1/7/2020 20:47'!initializeConFechaDeConstitucion: unaFecha monto: unMonto porcentajeDeInteres: unPorcentaje	"comment stating purpose of message"	fechaDeConstitucion := unaFecha.	montoDepositado := unMonto .	porcentajeDeInteresDiario := unPorcentaje.! !!PlazoFijo methodsFor: 'as yet unclassified' stamp: 'JulianCasaburi 9/22/2018 00:47'!valorActual	"comment stating purpose of message"	^(montoDepositado + ((montoDepositado / porcentajeDeInteresDiario ) * ((Date today  - fechaDeConstitucion ) days ))).! !!PlazoFijo methodsFor: 'accessing' stamp: 'JulianCasaburi 12/9/2019 18:12'!fechaDeConstitucion	^ fechaDeConstitucion ! !!PlazoFijo methodsFor: 'accessing' stamp: 'JulianCasaburi 12/9/2019 18:10'!montoDepositado	^ montoDepositado ! !!PlazoFijo methodsFor: 'accessing' stamp: 'JulianCasaburi 12/9/2019 18:10'!porcentajeDeInteresDiario	^ porcentajeDeInteresDiario ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PlazoFijo class	instanceVariableNames: ''!!PlazoFijo class methodsFor: 'instance creation' stamp: 'JulianCasaburi 1/7/2020 20:48'!conFecha: unaFecha conMonto: unMonto conPorcentaje: unPorcentaje	"comment stating purpose of message"	^self new		initializeConFechaDeConstitucion: unaFecha monto: unMonto porcentajeDeInteres: unPorcentaje;		yourself.! !TestCase subclass: #TestInversor	instanceVariableNames: 'inversor1 accion1 plazoFijo1'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-Ejercicio12'!!TestInversor methodsFor: 'tests' stamp: 'JulianCasaburi 12/9/2019 18:13'!test	| valorActualPF montoD valorPrevio |	inversor1 agregarInversion: accion1.		self assert: inversor1 valorActual closeTo: 20.0.		valorPrevio := inversor1 valorActual.		inversor1 agregarInversion: plazoFijo1.	montoD := plazoFijo1 montoDepositado. 	valorActualPF := (montoD + ((montoD / plazoFijo1 porcentajeDeInteresDiario ) * ((Date today  - plazoFijo1 fechaDeConstitucion ) days ))).	self assert: inversor1 valorActual closeTo: (valorPrevio + valorActualPF) .! !!TestInversor methodsFor: 'initialization' stamp: 'JulianCasaburi 9/22/2018 00:42'!setUp	"comment stating purpose of message"	inversor1 := Inversor conNombre: 'Inversor 1'.		accion1 := InversionEnAcciones		conNombre: 'Inversion Accion 1'		conCantidad: 2		conValorActualPorAccion: 10.			plazoFijo1 := PlazoFijo		conFecha: (Date year: 2018 month: 9 day: 20)		conMonto: 100.0		conPorcentaje: 10.	! !