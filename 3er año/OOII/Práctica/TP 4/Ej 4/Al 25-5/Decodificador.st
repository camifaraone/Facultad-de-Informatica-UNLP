Object subclass: #Criterio	instanceVariableNames: 'decodificador'	classVariableNames: ''	poolDictionaries: ''	category: 'Decodificador'!!Criterio commentStamp: 'Nicolas 5/25/2021 14:11' prior: 0!"Clase Criterio"!!Criterio methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 15:48'!decodificador	^ decodificador! !!Criterio methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 16:16'!decodificador: unDecodificador decodificador:= unDecodificador! !!Criterio methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 17:15'!peliculasRecientes	^ (self decodificador noReproducidas		asSortedCollection: [ :peli1 :peli2 | peli1 anio > peli2 anio ]) first: 3! !!Criterio methodsFor: 'as yet unclassified' stamp: 'Nicolas 5/25/2021 15:36'!criterioPeliculaself subclassResponsibility ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Criterio class	instanceVariableNames: ''!!Criterio class methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 16:17'!decodificador: unDecodificador	^ self new decodificador: unDecodificador; yourself ! !Object subclass: #Decodificador	instanceVariableNames: 'grilla reproducidas configuracion'	classVariableNames: ''	poolDictionaries: ''	category: 'Decodificador'!!Decodificador commentStamp: 'Nicolas 5/25/2021 17:05' prior: 0!"Clase Decodificador"!!Decodificador methodsFor: 'initialization' stamp: 'Nicolas 5/25/2021 15:28'!cambiarConfiguracion: unaConfiguracion	self configuracion: unaConfiguracion	! !!Decodificador methodsFor: 'initialization' stamp: 'Nicolas 5/25/2021 16:31'!initialize	configuracion := nil.	reproducidas := Set new.	grilla := Set new! !!Decodificador methodsFor: 'public' stamp: 'Nicolas 5/25/2021 15:19'!agregarPeliculaAGrilla: unaPelicula	grilla add: unaPelicula! !!Decodificador methodsFor: 'public' stamp: 'Nicolas 5/25/2021 15:31'!agregarPeliculaAReproducidas: unaPelicula	reproducidas  add: unaPelicula! !!Decodificador methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 15:29'!configuracion: unaConfiguracion	configuracion := unaConfiguracion! !!Decodificador methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 15:22'!reproducidas	^ reproducidas! !!Decodificador methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 15:22'!grilla	^ grilla! !!Decodificador methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 15:22'!configuracion	^ configuracion! !!Decodificador methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 16:34'!noReproducidas	^ self grilla reject:[:peli | self reproducidas includes: peli]! !Criterio subclass: #Novedad	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Decodificador'!!Novedad commentStamp: 'Nicolas 5/25/2021 14:12' prior: 0!"Clase Novedad"!!Novedad methodsFor: 'as yet unclassified' stamp: 'Nicolas 5/25/2021 16:35'!criterioPelicula	^ (self decodificador noReproducidas 		asSortedCollection: [ :peli1 :peli2 | peli1 anio > peli2 anio ])		first: 3! !Object subclass: #Pelicula	instanceVariableNames: 'titulo puntaje anio similitudes'	classVariableNames: ''	poolDictionaries: ''	category: 'Decodificador'!!Pelicula commentStamp: 'Nicolas 5/25/2021 14:24' prior: 0!"Clase Pelicula"!!Pelicula methodsFor: 'as yet unclassified' stamp: 'Nicolas 5/25/2021 16:07'!intiialize! !!Pelicula methodsFor: 'as yet unclassified' stamp: 'Nicolas 5/25/2021 16:08'!titulo: unTitulo puntaje: unPuntaje anio: unaAnio	titulo := unTitulo.	puntaje := unPuntaje.	anio := unaAnio.	similitudes := OrderedCollection new.! !!Pelicula methodsFor: 'as yet unclassified' stamp: 'Nicolas 5/25/2021 15:34'!agregarSimilitudes: unaPelicula	similitudes add: unaPelicula! !!Pelicula methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 15:46'!similitudes	^ similitudes! !!Pelicula methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 15:46'!anio	^ anio! !!Pelicula methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 15:46'!anio: anObject	anio := anObject! !!Pelicula methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 15:46'!titulo	^ titulo! !!Pelicula methodsFor: 'accessing' stamp: 'Nicolas 5/25/2021 15:46'!puntaje	^ puntaje! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pelicula class	instanceVariableNames: ''!!Pelicula class methodsFor: 'as yet unclassified' stamp: 'Nicolas 5/25/2021 14:51'!nuevaCon: unTitulo puntaje: unPuntaje anio: unAnio	^ self new		titulo: unTitulo		puntaje: unPuntaje		anio: unAnio! !Criterio subclass: #Puntaje	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Decodificador'!!Puntaje commentStamp: 'Nicolas 5/25/2021 14:24' prior: 0!"Clase Puntaje"!!Puntaje methodsFor: 'as yet unclassified' stamp: 'Nicolas 5/25/2021 17:19'!criterioPelicula	^ self peliculasRecientes		asSortedCollection: [ :peli1 :peli2 | peli1 puntaje >= peli2 puntaje ]! !Criterio subclass: #Similaridad	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Decodificador'!!Similaridad commentStamp: 'Nicolas 5/25/2021 14:24' prior: 0!Clase "Similaridad"!!Similaridad methodsFor: 'as yet unclassified' stamp: 'Nicolas 5/25/2021 17:31'!criterioPelicula	^ (self decodificador noReproducidas		select: [ :peli | self similaresReproducidas: peli ])	asSortedCollection: [ :peli1 :peli2 | peli1 anio > peli2 anio ]! !!Similaridad methodsFor: 'as yet unclassified' stamp: 'Nicolas 5/25/2021 17:29'!similaresReproducidas: unaPelicula	^ self decodificador reproducidas		contains: [ :peli | unaPelicula  similitudes includes: peli ]! !